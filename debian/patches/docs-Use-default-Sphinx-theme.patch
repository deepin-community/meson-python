 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/docs/conf.py b/docs/conf.py
index afe7d02..ccdc050 100644
--- a/docs/conf.py
+++ b/docs/conf.py
@@ -72,7 +72,7 @@ intersphinx_mapping = {'python': ('https://docs.python.org/3', None)}
